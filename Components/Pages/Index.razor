@page "/"

<h3>Index</h3>

@code {
    [Inject]
    public NavigationManager NavigationManager { get; set; } = default!;

    protected override Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            NavigationManager.NavigateTo("/Market");
        }

        return base.OnAfterRenderAsync(firstRender);
    }
}
